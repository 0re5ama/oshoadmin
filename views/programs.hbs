<div class="container-fluid" id='programForm'>
  <div class="row">
    <div class="col-md-12">
      <div class="card">
        <div class="card-header card-header-primary">
					<h4 class="card-title">\{{ addEdit }} Program</h4>
          <p class="card-category">Enter the Program details below</p>
        </div>
        <div class="card-body">
          <form>
            <div class="row">
              <div class="col-md-6">
								<div class="form-group">
									<label class="bmd-label-floating">Program</label>
									<select class="form-control" id="selProgram">
										<option v-for="option in programs" v-bind:value="option.id" v-model="selectedProg">\{{ option.name }}</option>
									</select>
								</div>
              </div>
							<div class="col-md-6">
								<div class="form-group">
									<label class="label-control">Venue</label>
									<select class="form-control" id="selVenue">
										<option v-for="option in venues" v-bind:value="option.id" v-model="selectedVenue">\{{ option.name }}</option>
									</select>
								</div>
              </div>
						</div>
						<div class="row">
              <div class="col-md-6">
                <div class="form-group">
                  <label class="bmd-label-floating">From Date</label>
									<input type="text" class="form-control datetimepicker futuredate" id="fromDate" />
                </div>
              </div>
              <div class="col-md-6">
                <div class="form-group">
                  <label class="bmd-label-floating">To Date</label>
									<input type="text" class="form-control datetimepicker futuredate" id="toDate" />
                </div>
              </div>
            </div>
            <div class="row">
              <div class="col-md-12">
                <div class="form-group">
                  <label class="bmd-label-floating">Acharya/Acharyashree</label>
									<select class="form-control" id="selAcharya" multiple="multiple">
										<option v-for="option in acharyas" v-bind:value="option.id" v-model="selectedAcharyas">\{{ option.name }}</option>
									</select>
                </div>
              </div>
            </div>
            <div class="row">
              <div class="col-md-12">
								<div class="form-group">
									<label class="bmd-label-floating">Remarks</label>
									<textarea class="form-control" rows="5"></textarea>
								</div>
              </div>
            </div>
						<button class="btn btn-primary pull-right" v-on:click="addProg" onclick="clog">\{{ addEdit }}</button>
            <div class="clearfix"></div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>
